import tensorflow as tf
import numpy as np

tensor_array = tf.TensorArray(dtype=tf.string, size=1, dynamic_size=True, clear_after_read=False)
# Example objects to be stored in the tensor array
object1 = tf.constant("Hello, TensorFlow!", dtype=tf.string)

# Write objects to the tensor array
tensor_array = tensor_array.write(0, object1)

# Read objects from the tensor array
read_object1 = tensor_array.read(0)

# Display the read objects
print("Object 1:", read_object1.numpy())
